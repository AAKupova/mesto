{
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": ["react"],
  "globals": {
    "document": true
  },
  "rules": {
    "import/prefer-default-export": 0
  }
}
